@Article{kusche2001,
  Title                    = {Diplopod {H}emocyanin {S}equence and the
 {P}hylogenetic {P}osition of the {M}yriapoda},
  Author                   = {Kristina Kusche and Thorsten Burmester},
  Journal                  = {Mol. Biol. Evol.},
  Year                     = {2001},
  Number                   = {8},
  Pages                    = {1566-1573},
  Volume                   = {18},

  Abstract                 = {Hemocyanins are copper-containing respiratory
 proteins of the Arthropoda that have so far been
 thoroughly investigated only in the Chelicerata and
 the Crustacea but have remained unstudied until now
 in the Myriapoda. Here we report the first sequence
 of a myriapod hemocyanin. The hemocyanin of
 Spirostreptus sp. (Diplopoda: Spirostreptidae) is
 composed of two distinct subunits that are arranged
 in a 6 x 6 native molecule. The cloned hemocyanin
 subunit c{DNA} codes of for a polypeptide of 653
 amino acids (75.5 kDa) that includes a signal
 peptide of 18 amino acids. The sequence closely
 resembles that of the chelicerate hemocyanins.
 Molecular phylogenetic analyses reject with high
 statistical confidence the integrity of the
 Tracheata (i.e., Myriapoda + Insecta) but give
 conflicting results on the position of the myriapod
 hemocyanin. While distance matrix and
 maximum-likelihood methods support a basal position
 of the Spirostreptus hemocyanin with respect to the
 other hemocyanins, parsimony analysis suggests a
 sister group relationship with the chelicerate
 hemocyanins. The latter topology is also supported
 by a unique shared deletion of an alpha-helix. A
 common ancestry of Myriapoda and Chelicerata should
 be seriously considered.},
  Groupsearch              = {0},
  Keywords                 = {Phylogeny; Myriapoda; Molecular methods}
}

@Article{kusche2003,
  Title                    = {Complete subunit sequences, structure and evolution
 of the 6 ? 6-mer hemocyanin from the common house
 centipede, {{\em Scutigera coleoptrata}}},
  Author                   = {Kristina Kusche and Anne Hembach and Silke
 Hagner-Holler and Wolfgang Gebauer and Thorsten
 Burmester},
  Journal                  = {Eur. J. Biochem.},
  Year                     = {2003},
  Pages                    = {2860-2868},
  Volume                   = {270},

  Abstract                 = {Hemocyanins are large oligomeric copper-containing
 proteins that serve for the transport of oxygen in
 many arthropod species. While studied in detail in
 the Chelicerata and Crustacea, hemocyanins had long
 been considered unnecessary in the Myriapoda. Here
 we report the complete molecular structure of the
 hemocyanin from the common house centipede Scutigera
 coleoptrata (Myriapoda: Chilopoda), as deduced from
 2D-gel electrophoresis, MALDITOF mass spectrometry,
 protein and c{DNA} sequencing, and homology
 modeling. This is the rst myriapod hemocyanin to be
 fully sequenced, and allows the investigation of
 hemocyanin structure function relationship and
 evolution. S. coleoptrata hemocyanin is a 6 ? 6-mer
 composed of four distinct subunit types that occur
 in an approximate 2 : 2 : 1 : 1 ratio and are 49.5
 55.5%identical. The c{DNA} of a fth, highly
 diverged, putative hemocyanin was identi ed that is
 not included in the native 6 ? 6-mer
 hemocyanin. Phylogenetic analyses show that myriapod
 hemocyanins are monophyletic, but at least three
 distinct subunit types evolved before the separation
 of the Chilopoda and Diplopoda more than 420 million
 years ago. In contrast to the situation in the
 Crustacea and Chelicerata, the substitution rates
 among the myriapod hemocyanin subunits are highly
 variable. Phylogenetic analyses do not support a
 common clade of Myriapoda and Hexapoda, whereas
 there is evidence in favor of monophyletic
 Mandibulata. Keywords: hemocyanin; subunit
 diversity; structure;Arthropoda; evolution.},
  Comment                  = {defence of Mandibulata from a molecular point of
 view},
  File                     = {KuscheEJB2003.pdf:/home/ward/biblio/Autres/KuscheEJB2003.pdf:PDF},
  Keywords                 = {Arthropoda Phylogeny}
}

