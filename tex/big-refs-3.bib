@Article{grandcolas2004b,
  Title                    = {The geometric mean length, a new statistic to
 describe the distribution of character steps on a
 tree},
  Author                   = {Philippe Grandcolas and Tony Robillard and Cyrille
 D'Haese and Laure Desutter-Grandcolas and Eric
 Guilbert and J\'er\^ome Murienne},
  Journal                  = {Cladistics},
  Year                     = {2004},
  Number                   = {3},
  Pages                    = {219-222},
  Volume                   = {20},

  Abstract                 = {The geometric mean length (GML) is proposed as a new
 statistic aimed at describing the evenness of
 character changes on a tree for a given set of
 character optimizations. It is the geometric mean of
 the number of steps on each branch of the tree,
 varying between a maximum value when all branch
 lengths are equal, and a minimum value when all
 branches but one have only one character step. It
 can be scaled according to its theoretical maximum
 value, thus indicating a relative GML that allows a
 comparison of the evenness of character steps
 between different tree topologies.},
  File                     = {GraRobDHaDesGuiMurCladistics2004.pdf:home/ward/biblio/mypapers/GraRobDHaDesGuiMurCladistics2004.pdf:PDF}
}

@InProceedings{grant2004,
  Title                    = {Insertions and deletions in the evolution of
